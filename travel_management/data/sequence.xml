<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for travel.customer -->
        <record id="seq_travel_order" model="ir.sequence">
            <field name="name">Travel Order</field>
            <field name="code">travel.customer</field>
            <field name="prefix">TB</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for travel.cpackage -->
        <record id="seq_package_order" model="ir.sequence">
            <field name="name">Travel Package</field>
            <field name="code">travel.package</field>
            <field name="prefix">TP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>